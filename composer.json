{
    "name": "toro/server-bundle",
    "type": "symfony-bundle",
    "description": "Server for Symfony2 projects.",
    "keywords": ["server"],
    "homepage": "https://github.com/intbizth/toro-server-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^7.0",
        "bcc/cron-manager-bundle": "^3.1",
        "liip/monitor-bundle": "^2.3",
        "snc/redis-bundle": "^2.0",
        "wrep/daemonizable-command": "1.3.*"
    },
    "require-dev": {
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Toro\\Bundle\\ServerBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Toro\\Bundle\\ServerBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
